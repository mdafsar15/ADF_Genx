<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CountriesView"
  Version="12.2.1.9.14"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  SelectList="CountryEO.COUNTRY_ID, 
       CountryEO.COUNTRY_NAME, 
       CountryEO.REGION_ID"
  FromList="COUNTRIES CountryEO"
  Where="REGION_ID = :bRegId">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.CountriesView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bRegId"
    Kind="where"
    Type="java.lang.Integer"/>
  <EntityUsage
    Name="CountryEO"
    Entity="model.CountryEO"/>
  <ViewAttribute
    Name="CountryId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CountryId"
    EntityUsage="CountryEO"
    AliasName="COUNTRY_ID"/>
  <ViewAttribute
    Name="CountryName"
    PrecisionRule="true"
    EntityAttrName="CountryName"
    EntityUsage="CountryEO"
    AliasName="COUNTRY_NAME"/>
  <ViewAttribute
    Name="RegionId"
    PrecisionRule="true"
    EntityAttrName="RegionId"
    EntityUsage="CountryEO"
    AliasName="REGION_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
