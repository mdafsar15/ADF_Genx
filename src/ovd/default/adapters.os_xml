                  <param name="groupbase" value="cn=groups,dc=jazn.com"/>
               </initParams>
            </plugin>
         </plugins>
         <default>
            <plugin name="DMSMetrics"/>
            <plugin name="NestedGroupMembership"/>
            <plugin name="JaznXmlProvider"/>
         </default>
      </pluginChains>
   </ghost>
</adapters>